eliminaMultiploLista(numero, lista):=block
(
	for i in lista do
	(
		if remainder (i,numero)=0 then
			delete(i, lista)
	),
	return (lista)
	
);
cribaAux(lista, listaRespuesta, nBase):=block
(
	if lista=[] then
		listaRespuesta
	else
		append(listaRespuesta, [nBase]),
		lista:eliminaMultiploLista(nBase, lista),
		print(lista),
		cribaAux(lista, listaRespuesta, first(lista))
);

criba(n):=block
(
	lista:makelist(i,i,2,n),
	return (cribaAux(lista, [], 2))
);

	